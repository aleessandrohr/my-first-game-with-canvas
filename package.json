{
	"name": "my-first-game-with-canvas",
	"version": "1.0.1",
	"type": "module",
	"main": "dist/server.js",
	"license": "MIT",
	"dependencies": {
		"express": "^4.17.1",
		"socket.io": "^3.1.2",
		"socket.io-client": "^3.1.2"
	},
	"devDependencies": {
		"@babel/core": "^7.13.10",
		"@babel/preset-env": "^7.13.10",
		"@types/express": "^4.17.11",
		"babelify": "^10.0.0",
		"browserify": "^17.0.0",
		"typescript": "^4.2.3"
	},
	"scripts": {
		"build": "yarn tsc",
		"bundle": "yarn bundle:client",
		"bundle:client": "browserify dist/client.js > dist/client-bundle.js -t babelify",
		"start": "yarn build && yarn bundle && yarn start",
		"start:server": "node dist/server.js",
		"update": "yarn upgrade-interactive --latest"
	}
}
